{
    "manifest_version": 2,

    "name": "Readmore Userscript",
    "description": "Fügt der deutschen eSport-Webseite zusätzliche Funktionen hinzu.",
    "version": "{{version}}",

    "permissions": [
        "https://readmore.de/",
        "https://www.readmore.de/",
        "https://readmore.thextor.de/"
    ],

    "content_scripts": [{
        "matches": ["https://readmore.de/*", "https://www.readmore.de/*", "https://userscript.thextor.de/*"],
        "js": ["readmore-userscript.min.user.js"],
        "run_at": "document_end"
    }],

    "author": "thextor, venyii, biki"
}